{
	"schemaVersion": 1,
	"id": "architect",
	"version": "${version}",
	"name": "STRUTTURA",
	"description": "Create and share your Minecraft structures with the world. Part of the STRUTTURA ecosystem.",
	"authors": [
		"MagiusCHE"
	],
	"contact": {
		"homepage": "https://struttura.magius.it",
		"sources": "https://github.com/MagiusCHE/struttura",
		"email": "magiusche@magius.it"
	},
	"license": "GPL-3.0-only",
	"icon": "assets/architect/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"it.magius.struttura.architect.Architect"
		],
		"client": [
			"it.magius.struttura.architect.ArchitectClient"
		],
		"modmenu": [
			"it.magius.struttura.architect.client.ModMenuIntegration"
		]
	},
	"mixins": [
		"architect.mixins.json",
		{
			"config": "architect.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "architect.accesswidener",
	"depends": {
		"fabricloader": ">=0.18.2",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"modmenu": "*"
	}
}